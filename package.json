{
  "name": "bind-calendar-picker",
  "version": "0.1.9",
  "description": "Calendar Web Component",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "@stencil/core": "^0.10.7",
    "@stencil/dev-server": "latest",
    "@stencil/utils": "latest",
    "@stencil/sass": "0.0.5",
    "@types/jest": "^23.3.0",
    "jest": "^23.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bind-doo/bind-calendar-picker.git"
  },
  "author": "Bind d.o.o.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bind-doo/bind-calendar-picker/issues"
  },
  "homepage": "https://www.bind.ba",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
